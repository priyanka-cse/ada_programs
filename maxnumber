#include <stdio.h>

int main(int argc, char **argv)
{
	int a[100],n,i,max;
	printf("Enter size\n");
	scanf("%d",&n);
	printf("Enter the %d elements\n",n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	max=a[0];
	for(i=0;i<n;i++)
	{
		if(max<a[i])
			max=a[i];
	}
	printf("%d is the maximum number\n",max);
	return 0;
}
 
